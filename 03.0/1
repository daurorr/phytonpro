a=[[],[],[],[],[],[],[],[],[]]
for i in range (9):
    for j in range (6):
        if (i%3==0):
            a[i].append('+')
        elif (i%3==1):
            a[i].append('*')
        elif (i%3==2):
            a[i].append('^')
for i in range (9):
    for j in range (0, 6):
        print(a[i][j], end=' ')
    print()
