a=[[],[],[],[],[],[],[]]
for i in range (7):
    for j in range (7):
        if (j%2==0) and (i%2==0):
            a[i].append(0)
        elif (j%2==1) and (i%2==1):
            a[i].append(0)
        elif (j%2==0) and (i%2==1):
            a[i].append(1)
        elif (j%2==1) and (i%2==0):
            a[i].append(1)
for i in range (7):
    for j in range (0, 7):
        print(a[i][j], end=' ')
    print()
