a=[[],[],[],[],[],[]]
for i in range (6):
    for j in range (6):
        elif (i%7==0  and j%7==2) or (i%7==0 and j%7==3) or (i%7==0 and j%7==4) or (i%7==0 and j%7==5) or (i%7==1  and j%7==5) (i%7==1  and j%7==3) (i%7==1  and j%7==4) :
            a[i].append(1)
        else:
            a[i].append(2)
for i in range (6):
    for j in range (0, 6):
        print(a[i][j], end=' ')
    print()
