num1="100"
num2="32"
num3="6"
num4=input("сколько молока купил?")
num5=input("сколько хлеба купил?")
num6=input("сколько сосисок купил?")
num1=int(num1)
num2=int(num2)
num3=int(num3)
num4=int(num4)
num5=int(num5)
num6=int(num6)
num7=num1*num4
num8=num2*num5
num9=num3*num6
num7=int(num7)
num8=int(num8)
num9=int(num9)
print(num7+num8+num9)
